{
  "name": "pow",
  "version": "3.8.8",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node .",
    "dev": "run-p build:debug:watch dev:run",
    "dev:run": "node --watch -r dotenv/config . dotenv_config_override=true",
    "build": "swc src --out-dir dist",
    "build:debug": "swc --source-maps=true src --out-dir=dist",
    "build:debug:watch": "swc --watch --source-maps=true src --out-dir=dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "check:types": "tsc",
    "prepare": "node .husky/install.mjs"
  },
  "packageManager": "pnpm@9.0.6",
  "engines": {
    "npm": "This project is configured to use pnpm"
  },
  "dependencies": {
    "@discordjs/voice": "0.16.1",
    "@sapphire/framework": "5.1.0",
    "@sapphire/plugin-subcommands": "6.0.3",
    "debug": "4.3.4",
    "discord-emoji": "2.4.0",
    "discord.js": "14.14.1",
    "emoji-regex": "10.3.0",
    "ffmpeg-static": "5.2.0",
    "mariadb": "3.3.0",
    "sodium-native": "4.1.1"
  },
  "devDependencies": {
    "@swc/cli": "0.3.12",
    "@swc/core": "1.5.2",
    "@tsconfig/node21": "21.0.3",
    "@tsconfig/strictest": "2.0.5",
    "@types/debug": "4.1.12",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/eslint__js": "8.42.3",
    "@types/node": "20.12.7",
    "chokidar": "3.6.0",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "globals": "15.1.0",
    "husky": "9.0.11",
    "npm-run-all2": "6.1.2",
    "prettier": "3.2.5",
    "typescript": "5.4.5",
    "typescript-eslint": "7.8.0"
  },
  "author": "kazukazu123123",
  "license": "MIT",
  "description": "",
  "type": "module"
}
