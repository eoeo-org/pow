    "build:debug:watch": "swc --watch --source-maps=true src --out-dir=dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "check:types": "tsc",
    "prepare": "node .husky/install.mjs"
