{
  "name": "pow",
  "version": "3.6.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "dev": "run-p build:debug:watch dev:run",
    "dev:run": "node --watch -r dotenv/config . dotenv_config_override=true",
    "build": "swc src --out-dir dist",
    "build:debug": "swc --source-maps=true src --out-dir=dist",
    "build:debug:watch": "swc --watch --source-maps=true src --out-dir=dist",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "check:types": "tsc",
    "prepare": "husky install || [ $? = 127 ]"
  },
  "packageManager": "pnpm@8.8.0",
  "engines": {
    "npm": "This project is configured to use pnpm"
  },
  "dependencies": {
    "@discordjs/collection": "1.5.3",
    "@discordjs/voice": "0.16.0",
    "@sapphire/framework": "4.6.0",
    "@sapphire/plugin-subcommands": "4.2.1",
    "debug": "4.3.4",
    "discord-emoji": "2.3.0",
    "discord.js": "14.13.0",
    "emoji-regex": "10.2.1",
    "ffmpeg-static": "5.2.0",
    "libsodium-wrappers": "0.7.13",
    "mariadb": "3.2.1"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.90",
    "@types/debug": "4.1.9",
    "@types/node": "20.7.1",
    "chokidar": "3.5.3",
    "dotenv": "16.3.1",
    "husky": "8.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "author": "kazukazu123123",
  "license": "MIT",
  "description": "",
  "type": "module"
}
